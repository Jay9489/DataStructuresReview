/******************************************************************************

UnsortedList isThere C++

*******************************************************************************/

#include <iostream>
 
using namespace std;

bool isThere(int[], int);

int main()
{
    bool a;
    int arr[] = {5,2,6,7,9,2,8,10,1,4};
    
    a = isThere(arr, 10);
    cout << a;
    return 0;
}
bool isThere(int arr1[], int size)
{
    // brute force
    int key = 10;
    for(int i = 0; i < size; i++)
    {
        if (key == arr1[i])
        {
            return true;
        }
    }
    return false;
    
    
}
